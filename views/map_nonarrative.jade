//This template is for map pages without a narrative along the right side
extends master

block css
	link(href="/css/map_nonarrative.css" rel="stylesheet")
	block map_css

block main
	div(id="mapWrapper")
		div(id="mapDiv")
			block map_div
	div(id="buttonDiv")
		div(class="btn-group-vertical" id="btn_group")
			block map_buttons

	div(id="narrative")
		div(id="sidebar")
			img(src="/images/und_logo_wide.png" alt="Univeristy of North Dakota wide flame logo" class="center-block hidden-xs hidden-sm img-responsive")
			img(src="/images/und_logo_compact.png" alt="University of North Dakota compact flame logo" class="center-block hidden-md hidden-lg img-responsive")
			p *Please allow the page to load completely before clicking the play buttons.

block footer
